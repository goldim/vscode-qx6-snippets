{
    "qx.Interface.define": {
        "prefix": ["qx.Interface.define"],
        "body": [
            "qx.Interface.define(\"${1:MyInterfaceName}\", {",
            "    members: {",
            "    }",
            "});"
        ],
        "description": "qooxdoo interface declaration"
    },
    "qx.Class.define": {
        "prefix": ["qx.Class.define"],
        "body": [
            "qx.Class.define(\"${1:MyClassName}\", {",
            "    extend: \"${2:qx.core.Object}\",",
            "",
            "    construct: function() {",
            "    },",
            "",
            "    properties: {",
            "    },",
            "",
            "    members: {",
            "    }",
            "});"
        ],
        "description": "qooxdoo class declaration"
    },
    "qx.Mixin.define": {
        "prefix": ["qx.Mixin.define"],
        "body": [
            "qx.Mixin.define(\"${1:MyMixinName}\", {",
            "    members: {",
            "    }",
            "});"
        ],
        "description": "qooxdoo mixin declaration"
    },
    "extend": {
        "prefix": ["extend"],
        "body": [
            "extend: \"${1:ClassName}\""
        ],
        "description": "Expects a parent class name"
    },
    "qooxdoo constructor": {
        "prefix": ["construct"],
        "body": [
            "construct: function($1) {",
            "    this.base(arguments);",
            "},"
        ],
        "description": "qooxdoo class constructor"
    },
    "qooxdoo destructor": {
        "prefix": ["destruct"],
        "body": [
            "destruct: function() {",
            "},"
        ],
        "description": "qooxdoo class destructor"
    }
}